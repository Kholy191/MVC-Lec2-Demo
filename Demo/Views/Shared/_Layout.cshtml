<!DOCTYPE html>

<html>
<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>@ViewBag.Title</title>
    <link rel="stylesheet" href="https://cdn.jsdelivr.net/npm/bootstrap@5.3.0/dist/css/bootstrap.min.css">
</head>
<body>
    @* Start Header *@
    <header class="mt-5 text-center">
        <nav class="navbar navbar-expand-lg navbar-light bg-light">
            <div class="container">
                <a href="/Home/Index" class="btn btn-primary">Home</a>
                <a href="/Home/AboutUs" class="btn btn-primary">About Us</a>
                <a href="/Home/Privacy" class="btn btn-primary">Privacy</a>
                <a href="/Home/ContactUs" class="btn btn-primary">Contact Us</a>
            </div>
        </nav>
    </header>
    <div>
        @RenderBody()
    </div>
    @* Footer *@
    <footer class="text-center mt-5">
        <div>
            <p>CopyRight @(DateTime.Now.Year)</p>
        </div>
    </footer>
</body>
</html>
